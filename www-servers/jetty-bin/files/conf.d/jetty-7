STOP_KEY=secret

# Set this value to 1 and configure the jetty-setuid.xml to have the server run as an unprivilaged user
#USE_SETUID=1

# Set this value to 1 to use ipv6
#USE_IPV6=1

# Use the SSL config jetty-ssl.xml
#USE_SSL=1

